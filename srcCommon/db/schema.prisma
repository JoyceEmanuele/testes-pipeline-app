model AUTOMATIONS_PERIODS {
  ID                             Int                             @id @default(autoincrement()) @db.UnsignedMediumInt
  AUTOMATION_PERIOD_STATUS       String?                         @db.Char(1)
  BEGIN_TIME                     String?                         @db.VarChar(5)
  END_TIME                       String?                         @db.VarChar(5)
