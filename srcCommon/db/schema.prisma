datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL") // DATABASE_URL="mysql://develbackend:uniquepassword@localhost:3306/dashdevelopment"
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL") // SHADOW_DATABASE_URL="mysql://develbackend:uniquepassword@localhost:3306/dashdevelopment_shadow"
}

generator client {
  provider = "prisma-client-js"
}

model ACCESS_DISTRIBUTORS {
  UNIT_ID                     Int       @id @db.UnsignedMediumInt
  ADDITIONAL_DISTRIBUTOR_INFO String?   @db.VarChar(100)
  CONSUMER_UNIT               String?   @db.VarChar(30)
  DISTRIBUTOR_ID              Int       @db.UnsignedMediumInt
  ENCRYPTED_PASSWORD          Int       @default(0) @db.TinyInt
  LOGIN                       String    @db.VarChar(175)
  LOGIN_EXTRA                 String?   @db.VarChar(175)
  PASSWORD                    String    @db.VarChar(250)
  STATUS                      String    @db.VarChar(30)
  STATUS_UPDATED_DATE         DateTime? @db.DateTime(0)

  DISTRIBUTORS DISTRIBUTORS @relation(fields: [DISTRIBUTOR_ID], references: [DISTRIBUTOR_ID])
  CLUNITS      CLUNITS      @relation(fields: [UNIT_ID], references: [UNIT_ID])
}

model ADDITIONAL_MACHINE_PARAMETERS {
  ID         Int      @id @default(autoincrement()) @db.UnsignedMediumInt
  MACHINE_ID Int      @db.UnsignedMediumInt
  NAME       String   @db.VarChar(100)
  VALUE      String   @db.VarChar(250)
  MACHINES   MACHINES @relation(fields: [MACHINE_ID], references: [ID])

  @@unique([MACHINE_ID, NAME])
}

model ADDITIONAL_NOBREAK_PARAMETERS {
  ID         Int      @id @default(autoincrement()) @db.UnsignedMediumInt
  NAME       String   @db.VarChar(100)
  NOBREAK_ID Int      @db.UnsignedInt
  VALUE      String   @db.VarChar(250)
  NOBREAKS   NOBREAKS @relation(fields: [NOBREAK_ID], references: [ID])

  @@unique([NOBREAK_ID, NAME])
}

model AIR_CURTAINS {
  ID         Int @id @default(autoincrement()) @db.UnsignedMediumInt
  ASSET_ID   Int @db.UnsignedInt
  MACHINE_ID Int @db.UnsignedMediumInt

  ASSETS   ASSETS   @relation(fields: [ASSET_ID], references: [ID])
  MACHINES MACHINES @relation(fields: [MACHINE_ID], references: [ID])
}

model ASSETS {
  ID                    Int       @id @default(autoincrement()) @db.UnsignedInt
  DAT_CODE              String?   @db.VarChar(50)
  INSTALLATION_DATE     DateTime? @db.Date
  INSTALLATION_LOCATION String?   @db.VarChar(250)
  MANUFACTURER          String?   @db.VarChar(100)
  MODEL                 String?   @db.VarChar(100)
  NAME                  String?   @db.VarChar(250)
  UNIT_ID               Int?      @db.UnsignedMediumInt

  CLUNITS                  CLUNITS?                @relation(fields: [UNIT_ID], references: [UNIT_ID])
  AIR_CURTAINS             AIR_CURTAINS[]
  ASSETS_HEALTH            ASSETS_HEALTH?
  ASSETS_HEALTH_HIST       ASSETS_HEALTH_HIST[]
  ASSETS_HEALTH_OBS        ASSETS_HEALTH_OBS[]
  ASSET_HEAT_EXCHANGERS    ASSET_HEAT_EXCHANGERS[]
  ASSET_IMAGES             ASSET_IMAGES[]
  CONDENSERS               CONDENSERS[]
  EVAPORATORS              EVAPORATORS[]
  NOBREAKS                 NOBREAKS[]
  CHILLERS                 CHILLERS[]
  CHILLER_CARRIER_MODELS   CHILLER_CARRIER_MODELS? @relation(fields: [cHILLER_CARRIER_MODELSID], references: [ID])
  cHILLER_CARRIER_MODELSID Int?                    @db.UnsignedInt
}

model ASSETS_HEALTH {
  ID                    Int                    @id @default(autoincrement()) @db.UnsignedMediumInt
  ASSET_ID              Int                    @db.UnsignedInt
  HEALTH_HIST_ID        Int                    @db.UnsignedMediumInt
  ASSETS                ASSETS                 @relation(fields: [ASSET_ID], references: [ID])
  ASSETS_HEALTH_HIST    ASSETS_HEALTH_HIST     @relation(fields: [HEALTH_HIST_ID], references: [ID])
  FAULTS_DATAS          FAULTS_DATAS?
  HEALTH_BEFORE_OFFLINE HEALTH_BEFORE_OFFLINE?

  @@unique([ASSET_ID])
}

model ASSETS_HEALTH_HIST {
  ID               Int              @id @default(autoincrement()) @db.UnsignedMediumInt
  DAT_REPORT       Int              @db.UnsignedInt
  ASSET_ID         Int              @db.UnsignedInt
  CT_ID            Int              @default(0)
  DAT_UNTIL        Int?             @db.UnsignedInt
  H_DATE           DateTime         @db.DateTime(0)
  H_DESC           String           @db.VarChar(250)
  H_INDEX          Int              @db.UnsignedInt
  P_CAUSE_ID       Int?             @db.UnsignedMediumInt
  ASSETS           ASSETS           @relation(fields: [ASSET_ID], references: [ID])
  TYPEHEALTHCHANGE TYPEHEALTHCHANGE @relation(fields: [CT_ID], references: [ID])
  P_CAUSES         P_CAUSES?        @relation(fields: [P_CAUSE_ID], references: [ID])
  ASSETS_HEALTH    ASSETS_HEALTH[]

  @@unique([DAT_REPORT, ASSET_ID, H_INDEX])
  @@index([DAT_REPORT])
}

model ASSETS_HEALTH_OBS {
  ID          Int    @id @default(autoincrement()) @db.UnsignedMediumInt
  ASSET_ID    Int    @db.UnsignedInt
  DAT_REPORT  Int    @db.UnsignedInt
  OBS_DESC    String @db.VarChar(250)
  USER_REPORT String @db.VarChar(50)

  ASSETS ASSETS @relation(fields: [ASSET_ID], references: [ID])
}

model ASSET_HEAT_EXCHANGERS {
  ID                         Int      @id @default(autoincrement()) @db.UnsignedMediumInt
  APPL_CFG                   String?  @db.VarChar(100)
  ASSET_ID                   Int      @db.UnsignedInt
  CAPACITY_POWER             Decimal? @db.Decimal(8, 2)
  CAPACITY_UNIT              String?  @db.VarChar(10)
  COMPRESSOR_NOMINAL_CURRENT Decimal? @db.Decimal(6, 2)
  EQUIPMENT_POWER            String?  @db.VarChar(150)
  EVAPORATOR_MODEL_ID        Int?     @db.UnsignedMediumInt
  HEAT_EXCHANGER_ID          Int?
  MACHINE_ID                 Int      @db.UnsignedMediumInt
  MACHINE_KW                 Decimal? @default(0.0000) @db.Decimal(8, 4)
  TYPE_CFG                   String?  @db.VarChar(100)

  ASSETS                     ASSETS                       @relation(fields: [ASSET_ID], references: [ID])
  EVAPORATORS_MODELS         EVAPORATORS_MODELS?          @relation(fields: [EVAPORATOR_MODEL_ID], references: [ID])
  HEAT_EXCHANGERS            HEAT_EXCHANGERS?             @relation(fields: [HEAT_EXCHANGER_ID], references: [ID])
  MACHINES                   MACHINES                     @relation(fields: [MACHINE_ID], references: [ID])
  DACS_ASSET_HEAT_EXCHANGERS DACS_ASSET_HEAT_EXCHANGERS?
  DRIS_ASSET_HEAT_EXCHANGERS DRIS_ASSET_HEAT_EXCHANGERS[]  
  DUTS_DUO_ASSET_HEAT_EXCHANGERS        DUTS_DUO_ASSET_HEAT_EXCHANGERS?
}

model ASSET_IMAGES {
  ID         Int    @id @default(autoincrement()) @db.UnsignedInt
  ASSET_ID   Int    @db.UnsignedInt
  DAT_UPLOAD Int    @db.UnsignedInt
  FILENAME   String @db.VarChar(100)

  ASSETS ASSETS @relation(fields: [ASSET_ID], references: [ID])
}

model ASSOCIATIONS {
  ID        Int    @id @default(autoincrement()) @db.UnsignedMediumInt
  CLIENT_ID Int    @db.UnsignedMediumInt
  NAME      String @db.VarChar(250)
  UNIT_ID   Int    @db.UnsignedMediumInt

  CLIENTS        CLIENTS          @relation(fields: [CLIENT_ID], references: [CLIENT_ID])
  CLUNITS        CLUNITS          @relation(fields: [UNIT_ID], references: [UNIT_ID])
  ASSOC_MACHINES ASSOC_MACHINES[]
}

model ASSOC_MACHINES {
  ID         Int @id @default(autoincrement()) @db.UnsignedMediumInt
  ASSOC_ID   Int @db.UnsignedMediumInt
  MACHINE_ID Int @db.UnsignedMediumInt
  POSITION   Int @db.UnsignedMediumInt

  ASSOCIATIONS ASSOCIATIONS @relation(fields: [ASSOC_ID], references: [ID])
  MACHINES     MACHINES     @relation(fields: [MACHINE_ID], references: [ID])
}

model AUTOMATIONS_PARAMETERS {
  ID                             Int                             @id @default(autoincrement()) @db.UnsignedMediumInt
  ACTION_MODE                    String?                         @db.VarChar(100)
  ACTION_POST_BEHAVIOR           String?                         @db.VarChar(100)
  ACTION_TIME                    Int?                            @db.UnsignedInt
  CURRENT_IN_DEVICE              String                          @default("0") @db.Char(1)
