}

model AUTOMATIONS_PERIODS_PARAMETERS {
  ID                       Int                    @id @default(autoincrement()) @db.UnsignedMediumInt
  AUTOMATION_PARAMETERS_ID Int                    @db.UnsignedMediumInt
  AUTOMATION_PERIOD_ID     Int                    @db.UnsignedMediumInt
  AUTOMATIONS_PARAMETERS   AUTOMATIONS_PARAMETERS @relation(fields: [AUTOMATION_PARAMETERS_ID], references: [ID])
  AUTOMATIONS_PERIODS      AUTOMATIONS_PERIODS    @relation(fields: [AUTOMATION_PERIOD_ID], references: [ID])

  @@unique([AUTOMATION_PARAMETERS_ID])
  @@unique([AUTOMATION_PERIOD_ID])
}

model AV_OPTS {
  OPT_ID    String  @id @db.VarChar(100)
  OPT_LABEL String  @db.VarChar(100)
  OPT_TYPE  String  @db.VarChar(100)
