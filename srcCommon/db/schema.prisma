}

model BASELINES {
  BASELINE_ID          Int                @id @default(autoincrement()) @db.UnsignedMediumInt
  BASELINE_TEMPLATE_ID Int                @db.UnsignedMediumInt
  UNIT_ID              Int                @db.UnsignedMediumInt
  BASELINE_TEMPLATES   BASELINE_TEMPLATES @relation(fields: [BASELINE_TEMPLATE_ID], references: [BASELINE_TEMPLATE_ID])
  CLUNITS              CLUNITS            @relation(fields: [UNIT_ID], references: [UNIT_ID])
